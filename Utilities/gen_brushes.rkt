(define println-define-syntax
  (lambda [color]
    (printf "static_read_only_property(Microsoft::Graphics::Canvas::Brushes::CanvasSolidColorBrush^, ~a);~n" (cadr color))))

(define println-implement-syntax
  (lambda [color]
    (printf (string-append "CanvasSolidColorBrush^ Colours::~a::get() {~n"
                           "    static CanvasSolidColorBrush^ brush = make_solid_brush(ColorHelper::FromArgb(255, ~a, ~a, ~a));~n~n"
                           "    return brush;~n"
                           "}~n~n")
            (cadr color) (caddr color) (cadddr color) (last color))))

(module+ main
  (define unique-colors (remove-duplicates named-colors bytes=? #:key cadr))
  
  (for-each println-define-syntax unique-colors)
  (newline)
  (for-each println-implement-syntax unique-colors))
